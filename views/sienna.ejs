<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <link rel="stylesheet" href="https://cdn.tutorialjinni.com/intl-tel-input/17.0.19/css/intlTelInput.css" />
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css"
    integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA=="
    crossorigin="anonymous" referrerpolicy="no-referrer" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/glider-js@1/glider.min.css" />

  <link rel="stylesheet" href="/profile/public/sienna/style.css" />
  <title>Xcard Theme</title>
</head>

<body>
  <div id="loader">
    <img src="/profile/public/sienna/assets/icons/loader.svg" alt="loading" />
  </div>

  <header>
    <img class="user_bg" id="user_bg" src="" alt="user-bg" />
    <div class="avatar_section">
      <div class="avatar_img">
        <img id="avatar" class="avatar" src="" alt="avatar" />
      </div>
      <div class="user_info">
        <div class="user_name_section">
          <h1 id="user_name" class="user_name">
            <!-- name -->
          </h1>
          <!-- <div class="rating d_none" id="rating_section" style="font-size: 3.5rem; padding: 10px 20px; display: flex; align-items: center;">
            <img src="https://services.google.com/fh/files/misc/google_g_icon_download.png" alt="star" width="50" />
            <p>Post Reviews</p>
          </div> -->
        </div>
      </div>
    </div>
  </header>

  

  <div class="contact_buttons" style="display: flex; justify-content: center; align-items: center; flex-wrap: wrap; gap: 10px; margin-top: 20px;">
    <button id="rating_button" class="btn btn_primary btn_dark" style="display: flex; align-items: center; padding: 10px 20px; gap: 8px; border: none; border-radius: 5px; background-color: #bf631c;">
        <img src="https://services.google.com/fh/files/misc/google_g_icon_download.png" alt="star" width="20" />
        <span style="color: white; font-size: 14px;">Post Reviews</span>
    </button>
    <button id="save_contact" class="btn btn_primary btn_dark" style="display: flex; align-items: center; padding: 10px 20px; gap: 8px; border: none; border-radius: 5px; background-color: #bf631c;">
        <span style="color: white; font-size: 14px;">Save Contact</span>
        <img src="/profile/public/sienna/assets/icons/download.svg" alt="download-icon" width="20" />
    </button>
    <button id="play_games" class="btn btn_primary btn_dark" style="display: flex; align-items: center; padding: 10px 20px; gap: 8px; border: none; border-radius: 5px; background-color: #bf631c;">
        <span style="color: white; font-size: 14px;">Play Games</span>
        <img src="https://cdn-icons-png.flaticon.com/512/13/13973.png" height="20" width="20" alt="Games-icon" style="filter: invert(1);" />
    </button>
</div>

  
  <div class="location d_none" id="location_display_id">
    <img src="/profile/public/sienna/assets/icons/location.svg" alt="location" />
    <p></p>
  </div>

  <div class="quote_section">
    <p id="bio">
      <!-- bio here -->
    </p>
  </div>



  <section id="products_section" class="products_section d_none">
    <div class="products_header">
      <h4 class="gradient_text sub_heading">Products</h4>
      <div class="dropdown">
        <select id="category" name="category">
          <option value="all" selected>Filter by category</option>
          <option value="all">All</option>
        </select>
      </div>
    </div>
    <div id="products_card_section" class="products_card_section">

    </div>
  </section>

  <section id="uncategorized_products" class="uncategorized_products d_none">
    <div class="uncategorized_products_slider_header">
      <h4 class="gradient_text sub_heading">Products</h4>
      <div class="uncategorized_glider_buttons">

        <button aria-label="Previous" class="btn uncategorized_products_glider_prev uncategorized_products_button">
          <img src="/profile/public/sienna/assets/icons/arrow_left_white.svg" alt="left-arrow" />
        </button>
        <button aria-label="Next" class="btn uncategorized_products_glider_next uncategorized_products_button">
          <img src="/profile/public/sienna/assets/icons/arrow_right_white.svg" alt="left-arrow" />
        </button>
      </div>
    </div>


    <div id="uncategorized_products_glider" class="uncategorized_products_cards uncategorized_products_glider glider draggable"></div>

    <div id="uncategorized_products_dots"></div>
  </section>



  <!-- documents section -->

  <section id="documents_section" class="documents_section">
    <h4 class="gradient_text sub_heading">Documents</h4>
    <div id="documents_cards" class="documents_cards"></div>
  </section>

  <section id="youtube_player_section" class="youtube_player_section">
  </section>

  <section id="contact_section" class="contact_section d_none">
    <h4 class="gradient_text sub_heading">Contact me</h4>
    <div id="contact_cards" class="contact_cards">

    </div>
  </section>

  <section id="user_contact_sites" class="user_contact_sites">
    <h4 class="gradient_text sub_heading">Websites</h4>

  </section>





  <!-- services section -->
  <section id="services_section" class="services_section">
    <div class="service_slider_header">
      <button aria-label="Previous" class="btn service_glider_prev">
        <img src="/profile/public/sienna/assets/icons/arrow_left.svg" alt="left-arrow" />
      </button>
      <h4 class="gradient_text sub_heading">Services</h4>
      <button aria-label="Next" class="btn service_glider_next">
        <img src="/profile/public/sienna/assets/icons/arrow_right.svg" alt="left-arrow" />
      </button>
    </div>


    <div id="service_glider" class="service_cards service_glider"></div>

    <div id="dots"></div>
  </section>

  <section id="certificate_section" class="certificate_section">
    <h4 class="gradient_text sub_heading">Certificates</h4>
    <div id="certificate_cards" class="certificate_cards">
      <!-- <div class="certificate_card">
                <img src="./assets/images/certificate.png" alt="certificate">
                <h5 class="gradient_text fw_600 f_16">Honorary Certificate</h5>
                <p class="fw_400 f_16">IT council</p>
            </div> -->
    </div>
  </section>

  <!-- <section id="bank_details_section" class="bank_details_section">
        <div class="bank_details_card">
            <h4 class="gradient_text sub_heading">Banking Details</h4>

            <div id="bank_detail_cards" class="bank_detail_cards">


            </div>
        </div>

    </section> -->

  <section id="awards_section" class="awards_section">
    <div class="awards_slider_header">
      <button aria-label="Previous" class="btn awards_glider_prev">
        <img src="/profile/public/sienna/assets/icons/arrow_left.svg" alt="left-arrow" />
      </button>
      <h4 class="gradient_text sub_heading">Awards</h4>
      <button aria-label="Next" class="btn awards_glider_next">
        <img src="/profile/public/sienna/assets/icons/arrow_right.svg" alt="left-arrow" />
      </button>
    </div>

    <div id="awards_cards" class="awards_cards awards_slider">
      <!-- <div class="award_card">
                <img class="award_img" src="./assets/images/headset.png" alt="product">
                <div class="product_details">
                    <h5 class="fw_600 f_16 gradient_text">Best Company 2023</h5>
                    <p class="fw_400 f_16">IT council</p>
                </div>
            </div>
            <div class="award_card">
                <img class="award_img" src="./assets/images/headset.png" alt="product">
                <div class="product_details">
                    <h5 class="fw_600 f_16 gradient_text">Best Company 2023</h5>
                    <p class="fw_400 f_16">IT council</p>
                </div>
            </div> -->
    </div>
  </section>

  <section class="enquiry_section">
    <h4 class="gradient_text sub_heading">ADD YOUR ENQUIRY</h4>
    <form class="enquiry_form">
      <input id="name_input" class="enquiry_input fw_400 f_16" type="text" placeholder="Enter your name" />
      <!-- phone input-->
      <div id="phone_input_wrapper" class="phone">
        <input type="text" class="phone fw_400 f_16" name="phone" id="phone" placeholder="Phone Number" />
      </div>

      <input id="email_input" class="enquiry_input fw_400 f_16" type="email" placeholder="Enter your email" />
      <textarea id="textarea" class="enquiry_input textarea fw_400 f_16" type="text" placeholder="Message"></textarea>
      <button id="enquiry_btn" class="enquiry_btn" type="submit">
        Submit
      </button>
    </form>
  </section>

  <footer>
    <h5 class="footer_text fw_400 f_12">
      Powered by
      <span class="footer_company_text fw_600 f_14">Skybertech</span>
    </h5>
    <a href="https://www.visitingcard.store/">
      <button class="btn footer_company_text">View more</button>
    </a>
  </footer>

  <!-- product popoup -->
  <div id="product_popup_section" class="product_popup_section popup d_none">
    <button id="product_popup_close_btn" class="btn popup_close_btn">
      <img src="/profile/public/sienna/assets/icons/close.svg" alt="close" />
    </button>
    <div class="popup_card">
      <img id="product_popup_img" class="popup_img" src="" alt="product" />
      <div class="popup_info">
        <h4 id="product_popup_heading" class="popup_heading fw_600">
          <!-- heading here -->
        </h4>
        <div class="popup_price_section">
          <p id="product_fake_price" class="fake_price text_dim">
            <!-- fake price here -->
          </p>
          <p id="product_popup_orginal_price" class="popup_orginal_price fw_600 f_16">
            <!-- orginal price here -->
          </p>
        </div>
        <p id="product_popup_desc" class="popup_desc text_dim fw_400 f_16">
          <!-- descreption here -->
        </p>
        <a id="product_popup_btn">
          <button class="popup_btn f_16 fw_600">Visit Store</button>
        </a>
      </div>
    </div>
  </div>

  <!-- service popup -->
  <div id="service_popup_section" class="service_popup_section popup d_none">
    <button id="service_popup_close_btn" class="btn popup_close_btn">
      <img src="/profile/public/sienna/assets/icons/close.svg" alt="close" />
    </button>
    <div class="popup_card">
      <img id="service_popup_img" class="popup_img" src="" alt="product" />
      <div class="popup_info">
        <h4 id="service_popup_heading" class="popup_heading fw_600">
          <!-- heading here -->
        </h4>
        <p id="service_popup_desc" class="popup_desc text_dim fw_400 f_16">
          <!-- descreption here -->
        </p>
        <a id="service_popup_btn">
          <button class="popup_btn f_16 fw_600">Visit Store</button>
        </a>
      </div>
    </div>
  </div>

  <!-- award popup -->
  <div id="award_popup_section" class="service_popup_section popup d_none">
    <button id="award_popup_close_btn" class="btn popup_close_btn">
      <img src="/profile/public/sienna/assets/icons/close.svg" alt="close" />
    </button>
    <div class="popup_card">
      <img id="award_popup_img" class="popup_img" src="" alt="product" />
      <div class="popup_info">
        <h4 id="award_popup_heading" class="popup_heading fw_600"></h4>
        <p id="award_popup_desc" class="popup_desc text_dim fw_400 f_16">
          <!-- descreption here -->
        </p>
      </div>
    </div>
  </div>

  <div id="certificate_image_section" class="popup d_none">
    <button id="popup_close_btn" class="btn popup_close_btn">
      <img src="/profile/public/sienna/assets/icons/close.svg" alt="close" />
    </button>
    <img class="certificate_popup_img" id="certificate_popup_img" src="" alt="certificate_popup_img" />
  </div>

  <!-- <button id="chatButton" class="">
  
    </button> -->
  <div id="largeDiv" class="largeDiv" style="
        position: fixed;
        bottom: 30px;
        width: 100%;
        display: flex;
        justify-content: center;
        align-content: center;
        align-items: center;
      "></div>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.19/js/intlTelInput.min.js"></script>

  <script>
    var input = document.querySelector("#phone");
    window.intlTelInput(input, {
      separateDialCode: true,
      initialCountry: "in",
    });
  </script>

  <script src="https://cdn.jsdelivr.net/npm/glider-js@1/glider.min.js"></script>
  <script defer src="/profile/public/sienna/script.js" data="<%= JSON.stringify(data) %>"  gamesEnabledPaths ="<%= JSON.stringify(gamesEnabledPaths) %>"></script>

  <script src="script.js"></script>
</body>

</html>